# Nästa Auth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=ditt_secret_här

# Database
DATABASE_URL="postgresql://användare:lösenord@localhost:5432/din_databas"

# Betalningsleverantör (exempel med Stripe)
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
STRIPE_PRICE_ID_MONTHLY=price_...
STRIPE_PRICE_ID_YEARLY=price_...

# E-post (för autentisering och notifieringar)
# För utveckling kan du använda Mailtrap, Ethereal, eller Amazon SES
# Besök https://nodemailer.com/smtp/well-known/ för andra tjänster
EMAIL_SERVER_USER=användarnamn
EMAIL_SERVER_PASSWORD=lösenord
EMAIL_SERVER_HOST=smtp.exempel.com
EMAIL_SERVER_PORT=587
EMAIL_FROM=no-reply@dindomän.com

# Supabase 
NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# URL-konfiguration
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Analytics
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=localhost